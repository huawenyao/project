# LangGraph Server 配置

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# LangSmith (可选 - 用于追踪和调试)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=ai-builder-studio

# 数据库配置
DATABASE_URL=postgresql://localhost:5432/ai_builder_studio

# Redis 配置
REDIS_URL=redis://localhost:6379

# 服务器配置
LANGGRAPH_SERVER_HOST=0.0.0.0
LANGGRAPH_SERVER_PORT=8123
LANGGRAPH_SERVER_WORKERS=4

# 安全配置
SECRET_KEY=your_secret_key_here
CORS_ORIGINS=http://localhost:12000,http://localhost:3000

# 日志级别
LOG_LEVEL=INFO
